<h1>Listing checkins</h1>

<table>
  <thead>
    <tr>
      <th>Studentid</th>
      <th>Date</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @checkins.each do |checkin| %>
      <tr>
        <td><%= checkin.studentid %></td>
        <td><%= checkin.date %></td>
        <td><%= link_to 'Show', checkin %></td>
        <td><%= link_to 'Destroy', checkin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Checkin', new_checkin_path %> <%= link_to 'Insert checkins by list', batch_checkin_path %>
